include "config.common"

---------------------------------------------------------------------
-- 启动配置
---------------------------------------------------------------------
--指定worker线程数量
thread      = 8
--启动守护进程模式，进程id保存到该文件中
--daemon    = "./test.pid"

--指定日志的输出目录
--logger  = "./log/test.log"
logpath   = "."

--启动的服务文件
start     = "main"
--支持最大的连接数
maxclient = 20480

---------------------------------------------------------------------
--自定义配置
---------------------------------------------------------------------
dbgport	  = 8888
http_port  = 8080
https_port = 8081



